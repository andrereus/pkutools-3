{
  "rules": {
    "communityFoods": {
      ".read": "auth != null",
      ".write": false,
      ".indexOn": ["language", "contributorId", "score"]
    },
    "$uid": {
      ".read": "$uid === auth.uid",
      ".write": false,
      "pheDiary": {
        ".indexOn": ["date"]
      },
      "labValues": {
        ".indexOn": ["date"]
      }
    }
  }
}
